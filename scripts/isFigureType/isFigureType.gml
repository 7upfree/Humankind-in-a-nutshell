function isFigureType(iid){
	if (iid == 64) {return true;}
	else if (iid == 65) {return true;}
	else if (iid == 66) {return true;}
	else if (iid == 67) {return true;}
	else if (iid == 68) {return true;}
	else if (iid == 69) {return true;}
	else if (iid == 70) {return true;}
	else if (iid == 71) {return true;}
	else if (iid == 72) {return true;}
	else if (iid == 73) {return true;}
	else {return false;}
}