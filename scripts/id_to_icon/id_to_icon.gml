// https://help.yoyogames.com/hc/en-us/articles/360005277377 for more information
function id_to_icon(iid){
	if (iid == 0)		{return icon00;}
	else if (iid == 1)	{return icon01;}
	else if (iid == 2)	{return icon02;}
	else if (iid == 3)	{return icon03;}
	else if (iid == 4)	{return icon04;}
	else if (iid == 5)	{return icon05;}
	else if (iid == 6)	{return icon06;}
	else if (iid == 7)	{return icon07;}
	else if (iid == 8)	{return icon08;}
	else if (iid == 9)	{return icon09;}
	else if (iid == 10)	{return icon10;}
	else				{return -1;}
}