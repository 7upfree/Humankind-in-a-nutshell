function cnt_rarityKor(rarityy){
	if (rarityy == 1)
	{
		return "일반"
	}
	else if (rarityy == 2)
	{
		return "희귀"
	}
	else if (rarityy == 3)
	{
		return "고급"
	}
	else if (rarityy == 4)
	{
		return "유일"
	}
	else if (rarityy == 5)
	{
		return "특별"
	}
	else if (rarityy == 10)
	{
		return "정착민"
	}
	else if (rarityy == 11)
	{
		return "사건"
	}
	else if (rarityy == 66)
	{
		return "적군"
	}
	else if (rarityy == 100)
	{
		return "위인"
	}
	else if (rarityy == 111)
	{
		return "전투 유닛"
	}
}